{
  "name": "capacitor-data-storage-sqlite",
  "version": "6.0.0",
  "description": "SQLite Storage of key/value strings pair",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "electron/",
    "CapacitorDataStorageSqlite.podspec"
  ],
  "author": "Jean Pierre QuÃ©au",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jepiqueau/capacitor-data-storage-sqlite.git.git"
  },
  "bugs": {
    "url": "https://github.com/jepiqueau/capacitor-data-storage-sqlite.git/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "capacitor-plugin",
    "capacitor-data-storage-sqlite",
    "capacitor-data-storage",
    "capacitor-storage",
    "capacitor-sqlite",
    "capacitor-sqlite-storage",
    "capacitor-sqlite-data-storage",
    "capacitor-sqlite-database",
    "capacitor-sqlite-db",
    "sqlite",
    "sqlite-database",
    "sqlite-db",
    "sqlite-storage",
    "sqlite-data-storage",
    "localforage",
    "local-storage",
    "local-data-storage",
    "data-storage",
    "storage",
    "database",
    "db",
    "key-value",
    "kv",
    "key-value-store",
    "kv-store",
    "key-value-storage",
    "kv-storage"
  ],
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "verify": "npm run verify:ios && npm run verify:android && npm run verify:web",
    "verify:ios": "cd ios && pod install && xcodebuild -workspace Plugin.xcworkspace -scheme Plugin -destination generic/platform=iOS && cd ..",
    "verify:android": "cd android && ./gradlew clean build test && cd ..",
    "verify:web": "npm run build",
    "verify:electron": "npm run build-electron",
    "lint": "npm run eslint && npm run prettier -- --check && npm run swiftlint -- lint",
    "fmt": "npm run eslint -- --fix && npm run prettier -- --write && npm run swiftlint --autocorrect --format",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier --config .prettierrc.js \"**/*.{css,html,ts,js,java}\"",
    "swiftlint": "node-swiftlint",
    "docgen": "docgen --api CapacitorDataStorageSqlitePlugin --output-readme docs/API.md --output-json dist/docs.json",
    "build": "npm run clean && npm run docgen && tsc && rollup -c rollup.config.mjs  && npm run build-electron",
    "build-electron": "tsc --project electron/tsconfig.json && rollup -c electron/rollup.config.mjs && rimraf ./electron/build",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "test": "echo \"No test specified\"",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/docgen": "^0.0.18",
    "@capacitor/ios": "^6.0.0",
    "@ionic/eslint-config": "^0.4.0",
    "@ionic/prettier-config": "^4.0.0",
    "@ionic/swiftlint-config": "^1.1.2",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "electron": "^31.2.1",
    "eslint": "^7.11.0",
    "prettier": "^3.3.3",
    "prettier-plugin-java": "^2.6.4",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.1",
    "swiftlint": "^1.0.2",
    "typescript": "~4.1.5"
  },
  "peerDependencies": {
    "@capacitor/core": "^6.0.0",
    "localforage": "^1.10.0"
  },
  "prettier": "@ionic/prettier-config",
  "swiftlint": "@ionic/swiftlint-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    },
    "electron": {
      "src": "electron"
    }
  },
  "dependencies": {
    "@types/node": "^18.19.40",
    "localforage": "^1.10.0"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}
